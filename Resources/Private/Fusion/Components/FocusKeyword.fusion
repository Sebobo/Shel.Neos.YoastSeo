prototype(Shel.Neos.YoastSeo:Component.FocusKeyword) < prototype(Shel.Neos.YoastSeo:Component.AbstractEditableField) {
    @context {
        fieldName = 'focusKeyword'
        fieldClass = 'focus-keyword'
    }

    content.@process.contentElementWrapping = Neos.Neos:ContentElementWrapping {
        node = ${documentNode}
    }
}
