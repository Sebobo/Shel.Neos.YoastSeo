prototype(Shel.Neos.YoastSeo:Component.PreviewUrl) < prototype(Neos.Fusion:Tag) {
    attributes {
        id = 'previewUrl'
        class = 'yoast-seo__previewUrl'
    }
    content = Neos.Fusion:UriBuilder {
        package = 'Shel.Neos.YoastSeo'
        controller = 'Page'
        action = 'renderPreviewPage'
        absolute = true
        additionalParams = Neos.Fusion:RawArray {
            node = ${documentNode.contextPath}
        }
    }
}
